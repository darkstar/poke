## Array instructions.

# Array creation.

instruction mka () # ULONG INT INT -> ARRB[N]
  code
    int type;
    int arrayof;
    uint64_t nelems;

    type = PVM_VAL_INT (JITTER_UNDER_TOP_STACK ());
    arrayof = PVM_VAL_INT (JITTER_TOP_STACK ());
    JITTER_DROP_STACK ();
    JITTER_DROP_STACK ();
    nelems = PVM_VAL_ULONG (JITTER_TOP_STACK ());
    
    JITTER_TOP_STACK () = pvm_make_array (type, arrayof, nelems);
  end
end

# Array setting and composition.

instruction aset () # ARR ULONG (INT|ARR) -> ARRI
  code
    uint64_t idx;
    pvm_val val;

    val= JITTER_TOP_STACK ();
    idx = PVM_VAL_ULONG (JITTER_UNDER_TOP_STACK ());
    JITTER_DROP_STACK ();
    JITTER_DROP_STACK ();

    /* XXX: Jump to Eabounds if index out of range.  */

    PVM_VAL_ARR_ELEM (JITTER_TOP_STACK (), idx) = val;
  end
end

# Array indexing.

instruction aref () # ARR ULONG -> (INT|ARR)
  code
    /* XXX: Jump to Eabounds if index out of range.  */
#define F(res, a, b) \
  { res = PVM_VAL_ARR_ELEM (a, PVM_VAL_ULONG (b)); }
    JITTER_BINARY_STACK(F);
#undef F
  end
end
