/* { dg-do run } */
/* { dg-options {-c "print if_2 (2UL, 8UL)" -c "print if_2 (4UL, 8UL)" -c "print if_2 (4UL, 10UL)" } } */


defun if_2 = (uint<64> a, uint<64> b) : uint<64>
{
   if (b == 0)
     return a;
   else
     return if_2 (b, a % b);
}

