/* { dg-do run } */

defun foo = int:
  {
   try
     raise 1UL;
   catch (Exception e)
   {
     if (e == E_div_by_zero)
       print ("got division by zero exception\n");
     else
       print ("got some other exception\n");
   }
   return 666;
  }

/* { dg-command { .print foo } } */
