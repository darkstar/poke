/* { dg-do run } */

defun foo = (Exception e) int:
  {
   try
     raise e;
   catch if E_div_by_zero
   {
     print ("catched\n");
   }

   return 0;
  }

defun bar = (Exception e) int:
  {
   try
     return foo (e);
   catch
   {
     print ("uncatched\n");
   }

   return 0;
  }

/* { dg-command {  bar (E_no_ios) } } */
/* { dg-command {  bar (E_div_by_zero) } } */
/* { dg-output "uncatched\n0\ncatched\n0" } */
