/* { dg-do run } */
/* { dg-options {-c "print loop_2 (0)" -c "print loop_2 (10)"} } */

defun loop_2 = (int i) : int
  {
   defvar n = 0;
   defvar res = 0;

   while (n < i)
     {
       if (n % 2 == 0)
         res = res + 1;

       n = n + 1;
     }

   return res;
   }

