/* { dg-do run } */

defun loop_2 = (int i) int:
  {
   defvar n = 0;
   defvar res = 0;

   while (n < i)
     {
       if (n % 2 == 0)
         res = res + 1;

       n = n + 1;
     }

   return res;
   }

