/* { dg-do run } */

defun foo = () : int
  {
   try
     raise E_div_by_zero;
   catch (Exception e)
   {
     if (e == E_div_by_zero)
       print ("got division by zero exception\n");
     else
       print ("got some other exception\n");
   }
   return 666;
  }

/* { dg-command {print foo ()} } */
/* { dg-output "got division by zero exception\n666" } */
