/* { dg-do run } */

deftype Callback = (int)int;

defun call_cb = (int[] a, Callback cb) int:
  {
   defvar sum = 0;

   for (e in a)
     sum = sum + cb (e);
   return sum;
  }

defun double = (int n) int: { return n * 2; }

