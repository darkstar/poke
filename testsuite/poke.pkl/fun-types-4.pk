/* { dg-do run } */

deftype Callback = (...)int;

defun add = (args...) int:
  {
   defvar res = 0;
   for (i in args) res = res + i as int;
   return res;
  }

defun apply = (Callback[] callbacks, int data) int:
  {
   defvar sum = 0;

   for (e in callbacks)
     sum = sum + e (data, 2, 3);
   return sum;
  }

/* { dg-command { apply ([add], 1) } } */
/* { dg-output "6" } */
  
